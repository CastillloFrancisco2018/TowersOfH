
public class Main 
{
	public static void main(String[] args) 
	{	
		String s = "hello";
		String s2 = "elephant";
		int theLengthOfs = s.length();
		int theLengthOfs2 = s2.length();
		
		System.out.println(theLengthOfs);
		System.out.println(theLengthOfs2);	
	}
	static int findLength(String a, String b)
	{
		int theLengthOfa = a.length();
		int theLengthOfb = b.length();
		
		return theLengthOfa;
		
		
	}
}


