public class Main
{
    public static void main(String[] args)
    {
        String s = "Matthew,Mark,Luke,John";
    }
    
    static void printParts(String s)
    {
        //Start with an empty string
        //go through the entire string
        //each character is either a comma or not
        //if its not a comma, concat to the string we are building
