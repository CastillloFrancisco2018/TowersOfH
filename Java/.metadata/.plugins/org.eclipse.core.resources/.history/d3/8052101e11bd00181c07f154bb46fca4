    static String reverseString2(String elephant)
    {
        String answer = "";
        
        for(int i = 0; i < elephant.length(); i += 1)
        {
            answer = answer + elephant.charAt(elephant.length() - 1 - i);
        }
        return answer;
    }
    
    static String reverseString(String elephant)
    {
        String answer = "";
        for(int i = elephant.length()-1; i >= 0; i = i - 1)
        {
            answer = answer + elephant.charAt(i);
        }
        return answer;
    }
}